{
  "name": "rest-example",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "scripts": {
    "start-client": "webpack-dev-server --history-api-fallback -d --hot --inline --no-info --port 3000 --host 0.0.0.0",
    "start-server": "nodemon src/api/index.js --watch src/api --exec babel-node",
    "start": "concurrently \"yarn run start-server\" \"yarn run start-client\"",
    "build": "webpack -p --progress --config ./webpack.config.js",
    "lint": "eslint src/",
    "test": "yarn run testonly && yarn run lint",
    "testonly": "mocha --compilers js:babel-core/register --reporter spec --full-trace ./src/**/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-accounts/rest-example"
  },
  "keywords": [],
  "author": "Tim Mikeladze",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "concurrently": "^3.1.0",
    "coveralls": "^2.11.15",
    "dotenv-webpack": "^1.3.1",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "html-webpack-plugin": "^2.24.1",
    "istanbul": "^1.1.0-alpha.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "watch-ignore-webpack-plugin": "^1.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "yarn": "^0.17.8"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.16.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^3.2.2",
    "react-router": "^3.0.0",
    "react-tap-event-plugin": "^2.0.1"
  }
