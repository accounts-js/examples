    "eslint-plugin-react": "^7.0.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul": "^1.1.0-alpha.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.17.2",
    "mocha": "^3.4.1",
    "nodemon": "^1.11.0",
    "watch-ignore-webpack-plugin": "^1.0.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.3"
  },
  "dependencies": {
    "@accounts/client": "^0.0.16",
    "@accounts/mongo": "^0.0.10",
    "@accounts/react": "^0.0.13",
    "@accounts/react-material-ui": "^0.0.13",
